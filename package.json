{
    "name": "@ecodev/natural-search",
    "version": "0.0.1",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "dev": "ng serve --port 1407 --open",
        "prod": "ng-packagr -p ng-package.json; gulp copy:themes",
        "prod:watch": "yarn prod; gulp watch",
        "docs": "git rm -rf --ignore-unmatch docs/; yarn prod; ng build -a demo-packaged --prod --aot --output-path docs --base-href /natural-search/ && cp docs/index.html docs/404.html && git add docs/ && git commit docs/ -m 'Update documentation'",
        "release": "yarn prod; cd node_modules/@ecodev/natural-search; yarn publish --access public",
        "llink": "cd node_modules/@ecodev/natural-search && yarn unlink && yarn link",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": false,
    "peerDependencies": {
        "@angular/common": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/material": "^5.2.5"
    },
