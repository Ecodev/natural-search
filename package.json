{
    "name": "demo",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "dev": "ng serve --port 4201",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build natural-search && cp -v README.md dist/natural-search/ && gulp copy:themes",
        "build-docs": "yarn build-lib && ng build demo --prod --aot --base-href /natural-search/ && cp dist/demo/index.html dist/demo/404.html",
        "release": "yarn build-lib && cd dist/natural-search/ && yarn publish --access public",
        "llink": "cd dist/natural-search && yarn unlink && yarn link"
    },
    "private": true,
    "dependencies": {
